	measure Tesoreria =
			
			var activo_corr = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=1100 &&
			                                            'Plan SI'[Numero]<1200))
			
			var inventario = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=1103 &&
			                                            'Plan SI'[Numero]<1104))
			
			var pasivo_corr = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2100 &&
			                                            'Plan SI'[Numero]<2200))
			
			var numerador = activo_corr-inventario
			RETURN
			DIVIDE(numerador, pasivo_corr,BLANK())
