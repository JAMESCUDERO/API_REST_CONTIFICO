	measure 'Fondo de maniobra' =
			
			var Activos_Corriente = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=1100 &&
			                                            'Plan SI'[Numero]<1200))
			var Pasivos_Corrientes = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2100 &&
			                                            'Plan SI'[Numero]<2200))
			RETURN
			 Activos_Corriente - Pasivos_Corrientes
		formatString: \$#,0;(\$#,0);\$#,0
		lineageTag: 0868e969-b452-4cde-b595-62f84208c982

	measure 'Fondo de maniobra (dias)' = ```
			
			var Activos_Corriente = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=1100 &&
			                                            'Plan SI'[Numero]<1200))
			
			var Pasivos_Corrientes = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2100 &&
			                                            'Plan SI'[Numero]<2200))
			
			VAR NUMERADOR = Activos_Corriente - Pasivos_Corrientes 
			
			VAR COSTO_VENTA = CALCULATE([YTD]
			                        ,FILTER('Plan RE','Plan RE'[Numero]>=5100
			                                        && 'Plan RE'[Numero]<5200))
			
			RETURN
			DIVIDE(NUMERADOR,COSTO_VENTA,BLANK())*365
			
			```
