	measure 'Tesoreria Neta' =
			
			var Disponible = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=1101 &&
			                                            'Plan SI'[Numero]<1102))
			var P_1 = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2104 &&
			                                            'Plan SI'[Numero]<2105))
			var P_2 = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2106 &&
			                                            'Plan SI'[Numero]<2107))
			var P_3 = CALCULATE(SUMX('Estado SI','Estado SI'[Valor])
			                                ,FILTER('Plan SI','Plan SI'[Numero]>=2101 &&
			                                            'Plan SI'[Numero]<2102))
			
			return
			
			Disponible
			+(P_1+P_2+P_3)
